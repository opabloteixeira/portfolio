generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Technologies {
  id          Int     @id @default(autoincrement())
  name        String
  description String
  img         String
  highlight   Boolean @default(false)

  @@map("technologies")
}
